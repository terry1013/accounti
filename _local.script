CREATE MEMORY TABLE T_SYSTEM_LOG(T_SLUSERID VARCHAR(10) NOT NULL,T_SLMILLIS BIGINT NOT NULL,T_SLDATETIME TIMESTAMP NOT NULL,T_SLFLAG VARCHAR(2) NOT NULL,T_SLTYPE VARCHAR(11) NOT NULL,T_SLMSGID VARCHAR(6) NOT NULL,T_SLMESSAGE VARCHAR(150) NOT NULL,T_SLDETAIL VARCHAR(16777216) NOT NULL,CONSTRAINT PK_T_SYSTEM_LOG PRIMARY KEY(T_SLUSERID,T_SLMILLIS))
CREATE MEMORY TABLE T_TASKS(T_TANAME VARCHAR(20) NOT NULL,T_TAID VARCHAR(10) NOT NULL,T_TADESCRIPTION VARCHAR(60) NOT NULL,T_TAICON BINARY NOT NULL,T_TATTASK VARCHAR(30) NOT NULL,T_TASTATUS VARCHAR(10) NOT NULL,T_TAPROGRESS INTEGER NOT NULL,T_TADATE_TIME TIMESTAMP NOT NULL,T_TACODE BINARY NOT NULL,AUDIT_TRACK VARCHAR(10) NOT NULL,CONSTRAINT PK_T_TASKS PRIMARY KEY(T_TANAME))
